<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.music.dao.MusicStyleDao">
    <resultMap id="rMap" type="MusicStyle">
        <id column="style_id" property="styleId"></id>
        <result column="style_name" property="styleName"></result>
    </resultMap>

    <select id="queryAllStyle" resultMap="rMap">
        SELECT * FROM music.music_style
    </select>

</mapper>