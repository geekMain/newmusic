<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.music.dao.SysLogDao">

    <insert id="addSysLog" parameterType="SysLog">
        insert into sys_log (user_id,login_device,sys_user_name,sys_name,login_browser,login_address,login_time,log_uuid)
        values
        (#{userId},#{loginDevice},#{sysUserName},#{sysName},#{loginBrowser},#{loginAddress},#{loginTime},#{logUUID})
    </insert>

    <select id="querySysLog" resultType="SysLog">
        select user_id as userId,
               login_device as loginDevice,
               sys_user_name as sysUserName,
               sys_name as sysName,
               login_browser as loginBrowser,
               login_address as loginAddress,
               login_time as loginTime,
           log_uuid as logUUID
        from sys_log
    </select>

    <insert id="addOperationUserLog" parameterType="UserOperationLog">
        insert into user_operation_log (log_uuid)
        values
        (#{logUUID})
    </insert>
</mapper>